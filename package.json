{
  "name": "swagpress",
  "version": "1.0.0",
  "description": "A boilerplate for Node.js web applications with express and MySQL",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:fresh": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "swagpress:seed": "sequelize-cli db:seed:all",
    "swagpress:generate-api": "node package/src/commands/SwaggerCommand.js",
    "swagpress:resource": "node package/src/commands/ResourceCommand.js --",
    "swagpress:make": "node package/src/commands/MakeCommand.js",
    "swagpress:docs": "node package/src/commands/DocsCommand.js"
  },
  "nodemonConfig": {
    "signal": "SIGTERM"
  },
  "author": "Mangesh Ghadigaonkar",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^3.1.7"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-session-sequelize": "^7.1.7",
    "cors": "^2.8.5",
    "csurf": "^1.10.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-flash": "0.0.2",
    "express-handlebars": "^8.0.1",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.3",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.3",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ulid": "^3.0.1",
    "validator": "^13.12.0"
  }
}
